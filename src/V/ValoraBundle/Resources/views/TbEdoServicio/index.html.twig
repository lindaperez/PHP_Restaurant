                    <li>
                        <a href="{{ path('EdoServicio_show', { 'id': entity.id }) }}">show/</a>
                        <a href="{{ path('EdoServicio_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul><br><br><br>
        <li>
            <a href="{{ path('EdoServicio_new') }}">
