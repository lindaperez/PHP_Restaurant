            <tr>
                <td align='center' width="80"><h1><strong>ID</strong></h1></td>
                <td width="5"></td>
                <td align='center' width="120"><h1><strong>Estatus</strong></h1></td>
                <td width="5"></td>
                <td align='center' width="120"><h1><strong>Acciones</strong></h1></td>
                
                
            </tr>
        </thead>
        <tbody>
